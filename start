#include<stdio.h>
#include<stdlib.h>
typedef struct node{
    int data;
    struct node *next;
}node;

int main(){
    node*first;
    node*second;
    node*third;
    first=(node*)malloc(sizeof(node));
    second=(node*)malloc(sizeof(node));
    third=(node*)malloc(sizeof(node));
    
    first->next=second;
    first->data=66;

    second->next=third;
    second->data=67;

    third->next=NULL;
    third->data=68;


    return 0;
    }

